{
  "name": "live-order-dashboard",
  "version": "0.0.0",
  "description": "Имитация админской панели, где в реальном времени приходят заказы (по WebSocket), а TanStack Query используется для загрузки и кэширования \"старых\" заказов и работы с серверными данными.",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "tsc",
    "format": "prettier --write .",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,json,css,md}": "prettier --write"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@tanstack/react-router": "^1.128.3",
    "@tanstack/react-router-devtools": "^1.128.3",
    "antd": "^5.27.0",
    "axios": "^1.10.0",
    "mobx": "^6.13.7",
    "mobx-react": "^9.2.0",
    "mobx-react-lite": "^4.1.0",
    "mobx-react-observer": "^1.1.0",
    "react": "^19.1.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@tanstack/router-plugin": "^1.128.3",
    "@types/node": "^24.0.14",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  }
}
